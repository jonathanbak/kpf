{
  "name": "jonathanbak/kpf",
  "description": "kubernetes php framework",
  "version": "1.1.0",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Jonathan Bak",
      "email": "jonathanbak@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "jonathanbak/mysqlilib": "~1.3",
    "twig/twig": "3.11.3",
    "symfony/deprecation-contracts": "^2.5",
    "symfony/polyfill-php80": "^1.27",
    "symfony/polyfill-php81": "^1.27",
    "symfony/polyfill-mbstring": "^1.27",
    "symfony/polyfill-ctype": "^1.27"
  },
  "conflict": {
    "twig/twig": ">=3.12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Kpf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KpfTest\\": "tests/"
    }
